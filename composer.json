{
    "name": "dimasriskifirdaus/package-skeleton",
    "friendly_name": "Package Skeleton",
    "description": "Package skeleton to develop a package for ProcessMaker 4",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PackageSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PackageSkeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PackageSkeleton\\PackageServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}